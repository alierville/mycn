{
  "name": "rdbc",
  "version": "0.1.0",
  "description": "Relational Database Connector",
  "main": "rdbc.min.js",
  "types": "rdbc.d.ts",
  "dependencies": {
    "sqlite3": "*"
  },
  "devDependencies": {
    "rollup": "0.56.5",
    "typescript": "2.7.2",
    "uglify-es": "3.3.9",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "_clear": "rimraf tsproject/compiled/*.js tsproject/compiled/*.d.ts",
    "_tsc": "tsc --project tsproject",
    "_make-bundle": "node tsproject/make-bundle",
    "build": "run-s _clear _tsc _make-bundle",
    "watch": "tsc --project tsproject --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/rdbc.git"
  },
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "SQLite",
    "pool",
    "connection",
    "transaction"
  ]
}